<idea-plugin>
    <id>cz.cuni.mff.kyjovsm.robocop</id>
    <name>Robocop</name>
    <vendor email="michalkyjovsky98@gmail.com" url="">Michal Kyjovsky</vendor>

    <description><![CDATA[
    Basic Robot Framework support for PyCharm.
    ]]></description>

    <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.python</depends>

    <extensions defaultExtensionNs="com.intellij">
        <fileType name="Robot File" implementationClass="cz.cuni.mff.kyjovsm.robocop.RobotFrameworkFileType" fieldName="INSTANCE"
                  language="RobotFramework" extensions="robot"/>
    </extensions>

    <actions>
        <!-- Add your actions here -->


    </actions>
</idea-plugin>